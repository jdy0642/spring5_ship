<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ship.web.adm.AdminMapper">
    
    <select id="selectByAdminIdPw" resultType="Admin" >
      	SELECT *
      	FROM ADMIN
      	WHERE AID = #{aid} AND UPW = #{upw}
        
    </select>
 
</mapper>